/* pages/search/search.wxss */

.search-box{
  box-sizing: border-box;
  position: fixed;
  width: 100%;
  height: 95rpx;
  z-index: 10;
  background-color: #79d397;
  display: flex;
  padding: 10rpx 30rpx;
}
.search-input{
  width: 550rpx;
  height: 70rpx;
  line-height: 70rpx;
  background-color: #fff;
  border-radius: 35rpx;
  padding-left: 50rpx;
  font-size: 30rpx;
}
.search-btn{
  flex: 1;
  color: #fff;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 30rpx;
  text-align: center;
}

.search-cont{
  padding-top: 70rpx;
}
.cont-none{
  position: relative;
}
.none-box{
  position: absolute;
  width: 100%;
  top:380rpx;
}
.none-img{
  display: block;
  width: 160rpx;
  height: 160rpx;
  margin: 0 auto;
  border-radius: 50%;
  box-shadow: 0 0 3rpx #666;
}
.none-text{
  font-size: 28rpx;
  color:#666;
  text-align: center;
  padding-top: 20rpx;
}